{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "project101-tst-adf"
		},
		"AzureKeyVault_tst_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://project101-tst-kv1.vault.azure.net/"
		},
		"Project101_AzureBlobStorage_TST_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "project101-tst-kv-secret"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_tst_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_tst_properties_typeProperties_baseUrl')]"
					},
					"Project101_AzureBlobStorage_TST_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('Project101_AzureBlobStorage_TST_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}